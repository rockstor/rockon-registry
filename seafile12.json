{
  "Seafile v12": {
    "containers": {
        "seafilev12-mariadb": {
            "image": "mariadb",
            "tag": "latest",
            "launch_order": 1,
            "volumes": {
                "/var/lib/mysql": {
                    "description": "MariaDB persistent data.",
                    "label": "MariaDB Storage [seafile-db]"
                }
            },
            "opts": [
                [
                    "-e",
                    "MYSQL_LOG_CONSOLE=true"
                ],
                [
                    "-e",
                    "MYSQL_ROOT_PASSWORD=MDA0NDRmNjYxOTMyMzJiOTY5MzZkNGY4"
                ]
            ]
        },
        "seafilev12-memcached": {
            "image": "memcached",
            "tag": "alpine",
            "launch_order": 2,
            "cmd_arguments": [
                [
                    "memcached",
                    "-m 256"
                ]
            ]
        },
        "seafilev12-mc": {
            "image": "seafileltd/seafile-mc",
            "tag": "12.0-latest",
            "launch_order": 3,
            "ports": {
                "80": {
                    "description": "Seafile WebUI port.",
                    "host_default": 8180,
                    "protocol": "tcp",
                    "label": "WebUI port [8180]",
                    "ui": true
                }
            },
            "volumes": {
                "/shared": {
                    "description": "Seafile persistent data.",
                    "label": "Seafile Storage [seafile-data]"
                }
            },
            "opts": [
                [
                    "-e",
                    "DB_HOST=mariadb"
                ],
                [
                    "-e",
                    "DB_ROOT_PASSWD=MDA0NDRmNjYxOTMyMzJiOTY5MzZkNGY4"
                ],
                [
                    "-e",
                    "SEAFILE_SERVER_LETSENCRYPT=false"
                ],
                [
                    "-e",
                    "JWT_PRIVATE_KEY=!Kz46gDb#waYHVAcFQAyhsEhbZ2aY)Mt"
                ],
                [
                    "--link",
                    "seafilev12-mariadb:mariadb"
                ],
                [
                    "--link",
                    "seafilev12-memcached:memcached"
                ]
            ]
        }
    },
    "version": "12.1",
    "description": "Secure file sharing and hosting.<p>HTTP only. To add HTTPS you can use Nginx-proxy-manager or follow SeaFiles instructions to integrate caddy.</p><p>Default username for your Seafile UI is <code>me@example.com</code>and password is<code>asecret</code></p><p>Based on the <a href='https://hub.docker.com/r/seafileltd/seafile-mc' target='_blank'>official OCI image</a>, <a href='https://hub.docker.com/_/memcached' target='_blank'>memcached</a>, <a href='https://hub.docker.com/_/mariadb' target='_blank'>mariadb</a>, and a <a href='https://hub.docker.com/r/lucaslorentz/caddy-docker-proxy' target='_blank'>custom caddy-docker-proxy</a>, available for amd64 and arm64 architecture.</p>",
    "icon": "https://avatars2.githubusercontent.com/u/1948782?s=400&v=4",
    "ui": {
        "slug": ""
    },
    "website": "https://www.seafile.com/"
  }
}