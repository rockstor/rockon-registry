{
  "Ubiquiti UniFi": {
    "description": "A containerized version of the Ubiquiti Network's UniFi Access Point controller.<p>Based on a <a href='https://hub.docker.com/r/jacobalberty/unifi' target='_blank'>custom OCI image</a>, available for amd64 and arm64 architecture.</p>",
    "version": "1.1",
    "website": "https://www.ui.com/",
    "containers": {
      "unifi": {
        "image": "jacobalberty/unifi",
        "launch_order": 1,
        "ports": {
          "3478": {
            "description": "Session Traversal Utilities for NAT port, udp.",
            "label": "STUN Port [e.g. 3478]",
            "host_default": 3478,
            "protocol": "udp"
          },
          "8080": {
            "description": "Port for UAP to inform controller (HTTP redirect for WebUI).",
            "label": "Redirect to WebUI [e.g. 8080]",
            "host_default": 8080,
            "protocol": "tcp",
            "ui": true
          },
          "8081": {
            "description": "Management Port.",
            "label": "Management Port [e.g. 8081]",
            "host_default": 8081,
            "protocol": "tcp"
          },
          "8443": {
            "description": "Port for WebUI and API.",
            "label": "HTTPS WebUI [e.g. 8443]",
            "host_default": 8443,
            "protocol": "tcp"
          },
          "8843": {
            "description": "Portal redirect port for HTTPs.",
            "label": "HTTPS Portal [e.g. 8843]",
            "host_default": 8843,
            "protocol": "tcp"
          },
          "8880": {
            "description": "Redirect to Portal.",
            "label": "Redirect to Portal [e.g. 8880]",
            "host_default": 8880,
            "protocol": "tcp"
          }
        },
        "volumes": {
          "/var/lib/unifi": {
            "description": "Share for UniFi Controller configuration.",
            "label": "Config Storage [e.g. unifi-config]"
          }
        }
      }
    }
  }
}
