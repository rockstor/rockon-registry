{
    "Gollum": {
        "description": "A Git Based Wiki on Alpine",
        "version": "0.0.1",
        "website": "https://github.com/gollum/gollum",
        "containers": {
            "gollum-alpine": {
                "image": "adtac/gollum-alpine",
                "launch_order": 1,
                "ports": {
                    "8080": {
                        "description": "Website Port",
                        "label": "Gollum Port",
                        "host_default": 8082,
                        "ui": true
                    }
                },
                "volumes": {
                    "/wiki": {
                        "description": "Choose a share for wiki storage",
                        "label": "Wiki storage"
                    }
                }
            }
        }
    }
}
