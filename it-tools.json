{
    "IT-TOOLS": {
        "description": "Locally hosted collection of handy online tools for developers and people working in IT (tools for cryptography, conversion, network, etc.).<p>Based on the <a href='https://hub.docker.com/r/corentinth/it-tools/' target='_blank'>official OCI image</a>, available for amd64 and arm64 architecture.</p>",
        "ui": {
            "https": false,
            "slug": ""
        },
        "volume_add_support": false,
        "website": "https://it-tools.tech/",
        "version": "1.0",
        "containers": {
            "it-tools": {
                "image": "corentinth/it-tools",
                "launch_order": 1,
                "ports": {
                    "80": {
                        "description": "WebUI port.",
                        "host_default": 18088,
                        "label": "WebUI port [e.g. 18088]}",
                        "protocol": "tcp",
                        "ui": true
                    }
                }
            }
        }
    }
}