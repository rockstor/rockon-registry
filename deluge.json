{
  "Deluge": {
    "description": "Deluge is a movie downloader for bittorrent users.<p>Based on a <a href='https://hub.docker.com/r/linuxserver/deluge' target='_blank'>custom OCI image</a>, available for amd64 and arm64 architecture.</p>",
    "version": "1.1",
    "website": "https://deluge-torrent.org/",
    "more_info": "Default username: admin<br>Default password: deluge.",
    "volume_add_support": true,
    "containers": {
      "Deluge": {
        "image": "linuxserver/deluge",
        "launch_order": 1,
        "ports": {
          "58846": {
            "description": "Deluge Daemon port.",
            "label": "Daemon port [e.g. 58846]",
            "host_default": 58846,
            "protocol": "tcp",
            "ui": true
          },
          "8112": {
            "description": "Deluge WebUI port.",
            "label": "WebUI port [e.g. 8112]",
            "host_default": 8112,
            "protocol": "tcp",
            "ui": true
          }
        },
        "volumes": {
          "/config": {
            "description": "Share for Deluge configuration.",
            "label": "Config Storage [e.g. Deluge-config]"
          },
          "/downloads": {
            "description": "Share for Deluge downloads.",
            "label": "Download Storage [e.g. Deluge-downloads]"
          }
        },
        "environment": {
          "PGID": {
            "description": "GID to use along with the above UID. It(or the above UID) must have full permissions to all Shares mapped in the previous step.",
            "label": "GID to run Deluge as.",
            "index": 2
          },
          "PUID": {
            "description": "UID to run Deluge as. It must have full permissions to all Shares mapped in the previous step.",
            "label": "UID to run Deluge as.",
            "index": 1
          }
        }
      }
    }
  }
}
