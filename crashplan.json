{
    "Crashplan": {
        "website": "https://hub.docker.com/r/jrcs/crashplan/",
        "version": "1.0",
        "description": "Crashplan is a cloud and local backup solution",
        "containers": {
            "crashplan": {
                "image": "jrcs/crashplan",
                "tag": "latest",
                "launch_order": 1,
                "volume_add_support": true,
                "ports": {
                    "4242": {
                        "description": "Crashplan Communication Port. Suggested default: 4242",
                        "host_default": 4242,
                        "label": "Comm port 1",
                        "protocol": "tcp",
                        "ui": true
                    },
                    "4243": {
                        "description": "Crashplan Communication Port. Suggested default: 4243",
                        "host_default": 4243,
                        "label": "Comm port 2",
                        "protocol": "tcp",
                        "ui": true
                    }
                },
                "volumes": {
                    "/var/crashplan": {
                        "description": "Choose a Share for crashplan Configuration Files",
                        "label": "Config Storage"
                    }
                }
            }
        }
    }
}
