{
  "Paperless-ngx": {
    "description": "Paperless DMS - a private & secure Document Management System. <p>Based on the <a href='https://github.com/paperless-ngx/paperless-ngx/pkgs/container/paperless-ngx' target=_blank>official OCI image</a>, available for amd64 and arm64 architecture.</p>",
    "version": "1.5",
    "website": "https://docs.paperless-ngx.com/",
    "volume_add_support": true,
    "containers": {
      "paperless": {
        "image": "ghcr.io/paperless-ngx/paperless-ngx",
        "launch_order": 3,
        "ports": {
          "8000": {
            "description": "Port for web interface.",
            "label": "WebUI port [e.g. 8420]",
            "host_default": 8420,
            "protocol": "tcp",
            "ui": true
          }
        },
        "volumes": {
          "/usr/src/paperless/consume": {
            "description": "Share for importing documents.",
            "label": "Import [e.g. paperless-import]"
          },
          "/usr/src/paperless/data": {
            "description": "Share for data file storage.",
            "label": "Data [e.g. paperless-data]"
          },
          "/usr/src/paperless/export": {
            "description": "Share for exported documents.",
            "label": "Export [e.g. paperless-export]"
          },
          "/usr/src/paperless/media": {
            "description": "Share for media storage.",
            "label": "Media [e.g. paperless-media]"
          }
        },
        "opts": [
          [
            "-e",
            "PAPERLESS_DBHOST=paperless-db"
          ],
          [
            "-e",
            "PAPERLESS_REDIS=redis://paperless-broker:6379"
          ]
        ],
        "environment": {
          "PAPERLESS_ADMIN_MAIL": {
            "description": "Specify superuser email address.",
            "label": "Superuser Mail address [e.g. root@localhost]",
            "index": 2
          },
          "PAPERLESS_ADMIN_PASSWORD": {
            "description": "This will be the password of the automatically created superuser.",
            "label": "Superuser password",
            "index": 3
          },
          "PAPERLESS_ADMIN_USER": {
            "description": "Enter the username for the paperless-ngx superuser.",
            "label": "Superuser username [e.g. proot]",
            "index": 1
          }
        }
      },
      "paperless-broker": {
        "image": "docker.io/library/redis",
        "tag": "7",
        "launch_order": 1,
        "volumes": {
          "/data": {
            "description": "Share for Redis data.",
            "label": "Redis Data [e.g. paperless-redis]"
          }
        }
      },
      "paperless-db": {
        "image": "docker.io/library/postgres",
        "tag": "16",
        "launch_order": 2,
        "volumes": {
          "/var/lib/postgresql/data": {
            "description": "Share for PostgreSQL database storage.",
            "label": "PostgreSQL DB [e.g. paperless-db]"
          }
        },
        "opts": [
          [
            "-e",
            "POSTGRES_DB=paperless"
          ],
          [
            "-e",
            "POSTGRES_USER=paperless"
          ],
          [
            "-e",
            "POSTGRES_PASSWORD=paperless"
          ]
        ]
      }
    },
    "container_links": {
      "paperless": [
        {
          "name": "paperless-to-broker",
          "source_container": "paperless-broker"
        },
        {
          "name": "paperless-to-paperless-db",
          "source_container": "paperless-db"
        }
      ]
    }
  }
}
