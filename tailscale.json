{
    "Tailscale": {
        "containers": {
            "launch_order": 1,
            "tailscaled": {
                "image": "tailscale/tailscale",
                "tag": "stable",
                "opts": [
                    [
                        "--net=host",
                        ""
                    ],
                    [
                        "--privileged",
                        ""
                    ],
                    [
                        "-v",
                        "/var/lib:/var/lib"
